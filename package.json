{
  "name": "@dryan-llc/mnml.css",
  "version": "6.5.0",
  "description": "",
  "main": "mnml.css",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "prebuild": "npm run clean",
    "build": "npx browserslist@latest --update-db && npx ts-node -O '{\"module\":\"commonjs\"}' ./builder.cts",
    "clean": "rm -rf dist/*",
    "docs": "npm run build && cp ./dist/mnml* ./docs/assets/.",
    "watch": "nodemon --exec npm run build",
    "watch:docs": "nodemon --exec npm run docs",
    "prepublish": "npm run build"
  },
