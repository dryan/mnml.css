  "license": "MPL",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "browserslist": "^4.19.1",
    "cssnano": "^6.0.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.21",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.2.0",
    "prettier": "^2.8.7",
    "prettier-plugin-css-order": "^1.3.0"
  },
  "files": [
    "mnml*.css"
  ],
  "prettier": {
    "order": "alphabetical"
  }
}
