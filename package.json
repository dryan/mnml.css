{
  "name": "@dryan-llc/mnml.css",
  "version": "4.0.3",
  "description": "",
  "main": "dist/mnml.css",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "prebuild": "npm run clean",
    "build": "npx browserslist@latest --update-db && npx ts-node -O '{\"module\":\"commonjs\"}' ./builder.ts",
    "clean": "rm -rf dist/*",
    "docs": "npm run build && cp ./dist/mnml* ./docs/assets/.",
    "watch": "nodemon --exec npm run build",
    "watch:docs": "nodemon --exec npm run docs",
    "prepublish": "npm run build"
  },
  "author": "Dan Ryan",
  "license": "MPL",
  "devDependencies": {
    "@parcel/css": "^1.13.0",
    "@types/node": "^17.0.14",
    "browserslist": "^4.19.1",
    "nodemon": "^2.0.15",
    "open-color": "^1.9.1",
    "postcss": "^8.4.21",
    "prettier-plugin-css-order": "^1.3.0"
  },
  "files": [
    "dist/**/*"
  ],
  "prettier": {
    "order": "alphabetical"
  }
}
